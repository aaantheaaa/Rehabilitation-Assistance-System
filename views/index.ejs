    <script>
        
        if (sessionStorage.getItem("login") == null) {
            sessionStorage.setItem("login", "-1"); // -1 means not logged in, it logged in it will be id number (positive number)
        }
        var login = sessionStorage.getItem("login");        
        if (login != "-1") {
            document.getElementById("signinButton").style.display = "none";
            document.getElementById("signupButton").style.display = "none";
            document.getElementById("logoutButton").style.display = "block";
            document.getElementById("trainButton").style.display = "block";
        } 
        function logout() {
            sessionStorage.setItem("login", "-1");
            document.getElementById("signinButton").style.display = "block";
            document.getElementById("signupButton").style.display = "block";
            document.getElementById("logoutButton").style.display = "none";
            document.getElementById("trainButton").style.display = "none";
